{
  "name": "weapp",
  "version": "1.6.19",
  "private": true,
  "description": "哈德韦的个人小程序",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "stylus"
  },
  "sideEffects": [
    "./src/layout/fab.styl",
    "./src/pages/yuque/article.styl"
  ],
  "scripts": {
    "build:weapp": "taro build --type weapp --open",
    "build:weapp:upload": "taro build --type weapp --upload",
    "build:weapp:preview": "taro build --type weapp --preview",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:h5:windows": "set NODE_OPTIONS=\"--openssl-legacy-provider\"&&taro build --type h5",
    "build:weapp:windows": "set NODE_OPTIONS=\"--openssl-legacy-provider\"&&taro build --type weapp",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:h5:windows": "npm run build:h5:windows -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest --config ./jest.config.js --no-cache --coverage",
    "deeplinks": "node scripts/deeplinks.js",
    "serve": "python3 -m http.server --directory dist --cgi",
    "node18": "NODE_OPTIONS=\"--no-experimental-fetch --openssl-legacy-provider\" yarn build:h5",
    "update": "taro update self && taro update project"
  },
  "taroConfig": {
    "version": "1.6.18",
    "desc": "哈德韦的个人小程序"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "Jeff Tian <jeff.tian@outlook.com>",
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@authing/guard-react": "^5.1.2",
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.6.7",
    "@tarojs/components-rn": "^3.6.17",
    "@tarojs/plugin-framework-react": "3.6.7",
    "@tarojs/plugin-platform-h5": "3.6.7",
    "@tarojs/react": "3.6.7",
    "@tarojs/rn-runner": "^3.6.17",
    "@tarojs/rn-supporter": "^3.6.17",
    "@tarojs/runtime": "3.6.7",
    "@tarojs/runtime-rn": "^3.6.17",
    "@tarojs/taro": "3.6.7",
    "@tarojs/taro-rn": "^3.6.17",
    "apollo-upload-client": "^17.0.0",
    "authing-wxapp-sdk": "^4.3.22-alpha.7",
    "eval5": "^1.4.7",
    "expo": "~49.0.0",
    "form-urlencoded": "^6.0.5",
    "graphql": "^15.6.0",
    "mdast-util-to-string": "^3.1.0",
    "querystring": "^0.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.73.0",
    "remark": "5.0.1",
    "remark-html": "11.0.0",
    "taro-ui": "3.1.0-beta.4",
    "transform-css-to-js": "^0.1.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.8.0",
    "@babel/helper-get-function-arity": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@tarojs/cli": "3.6.7",
    "@tarojs/mini-runner": "3.6.7",
    "@tarojs/plugin-mini-ci": "3.6.7",
    "@tarojs/webpack-runner": "3.6.7",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.6.7",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.6.7",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "metrohash": "^2.8.0",
    "react-refresh": "^0.14.0",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
