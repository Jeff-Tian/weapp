{
  "name": "weapp",
  "version": "1.6.0",
  "private": true,
  "description": "增加 Keycloak 对接",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "stylus"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp --open",
    "build:weapp:upload": "taro build --type weapp --upload",
    "build:weapp:preview": "taro build --type weapp --preview",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:h5:windows": "npm run build:h5:windows -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest --config ./jest.config.js --no-cache --coverage",
    "wmp:upload": "node scripts/upload.js",
    "deeplinks": "node scripts/deeplinks.js",
    "serve": "python3 -m http.server --directory dist --cgi",
    "node18": "NODE_OPTIONS=\"--no-experimental-fetch --openssl-legacy-provider\" yarn build:h5"
  },
  "taroConfig": {
    "version": "1.5.5",
    "desc": "上传描述"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "Jeff Tian <jeff.tian@outlook.com>",
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.4.13",
    "@tarojs/plugin-framework-react": "^3.4.13",
    "@tarojs/react": "3.4.13",
    "@tarojs/runtime": "3.4.13",
    "@tarojs/taro": "3.4.13",
    "authing-wxapp-sdk": "^4.3.22-alpha.7",
    "cheerio": "^1.0.0-rc.10",
    "eval5": "^1.4.7",
    "form-urlencoded": "^6.0.5",
    "graphql": "^15.6.0",
    "jest": "^27.2.5",
    "mdast-util-to-string": "^3.1.0",
    "querystring": "^0.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "remark": "5.0.1",
    "remark-html": "5.0.1",
    "taro-ui": "3.1.0-beta.4",
    "transform-css-to-js": "^0.1.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "3.4.13",
    "@tarojs/mini-runner": "3.4.13",
    "@tarojs/plugin-mini-ci": "3.4.13",
    "@tarojs/webpack-runner": "3.4.13",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.4.13",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.4.13",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "miniprogram-ci": "^1.6.10",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0"
  }
}
